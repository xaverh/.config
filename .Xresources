Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

URxvt.intensityStyles: true
URxvt*geometry: 86x24
URxvt*scrollBar: false
URxvt*font: xft:SFMono:pixelsize=13
URxvt*italicFont: xft:SFMono:pixelsize=13:italic
URxvt*boldFont: xft:SFMono:pixelsize=13:bold
URxvt*boldItalicFont: xft:SFMono:pixelsize=13:bold:italic
URxvt*letterSpace: -2
! URxvt*font: -sgi-screen-medium-r-*-*-13-*-*-*-*-*-*-*
! URxvt*boldFont: -sgi-screen-bold-r-*-*-13-*-*-*-*-*-*-*
URxvt.internalBorder:  14
URxvt*inheritPixmap: false
! URxvt*transparent:   true
! URxvt*shading: 15

*scrollColor:        	#FFFFFF
*cursorBlink:			1

! special
*.foreground:   #d4d4d4
*.background:   #1e1e1e
*.cursorColor:  #d4d4d4

! black
*.color0:       #252526
*.color8:       #454545

! red
*.color1:       #f44747
*.color9:       #d16969

! green
*.color2:       #608b4e
*.color10:      #b5cea8

! yellow
*.color3:       #ce9178
*.color11:      #d7ba7d

! blue
*.color4:       #6796e6
*.color12:      #9cdcfe

! magenta
*.color5:       #646695
*.color13:      #c586c0

! cyan
