Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

URxvt.intensityStyles: true
URxvt*geometry: 86x24
URxvt*scrollBar: false
URxvt*font: xft:SFMono:pixelsize=13
URxvt*italicFont: xft:SFMono:pixelsize=13:italic
URxvt*boldFont: xft:SFMono:pixelsize=13:bold
URxvt*boldItalicFont: xft:SFMono:pixelsize=13:bold:italic
URxvt*letterSpace: -2
! URxvt*font: -sgi-screen-medium-r-*-*-13-*-*-*-*-*-*-*
! URxvt*boldFont: -sgi-screen-bold-r-*-*-13-*-*-*-*-*-*-*
URxvt.internalBorder:  14
URxvt*inheritPixmap: false
! URxvt*transparent:   true
! URxvt*shading: 15

*scrollColor:        	#FFFFFF
*cursorBlink:			1

! special
*.foreground:   #f2f0c9
*.background:   #1e1e1e
*.cursorColor:  #f2f0c9

! black
*.color0:       #1e1e1e
*.color8:       #373b41

! red
*.color1:       #a62844
*.color9:       #b0535e

! green
*.color2:       #406847
*.color10:      #7ba886

! yellow
*.color3:       #f2b33d
*.color11:      #fce779

! blue
*.color4:       #2484bf
*.color12:      #5986c9

! magenta
*.color5:       #6c63a6
*.color13:      #9393b0

! cyan
*.color6:       #81d8d0
*.color14:      #bfe3e3

! white
*.color7:       #606073
*.color15:      #f2f0c9

