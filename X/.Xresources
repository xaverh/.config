Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

URxvt.intensityStyles: true
URxvt*geometry: 86x24
URxvt*scrollBar: false
URxvt*font: xft:PragmataProMono:pixelsize=16
URxvt*italicFont: xft:PragmataProMono:pixelsize=16:italic
URxvt*boldFont: xft:PragmataProMono:pixelsize=16:bold
URxvt*boldItalicFont: xft:PragmataProMono:pixelsize=16:bold:italic
! URxvt*lineSpace: 3
! URxvt*font: xft:Menlo:pixelsize=13
! URxvt*italicFont: xft:Menlo:pixelsize=13:italic
! URxvt*boldFont: xft:Menlo:pixelsize=13:bold
! URxvt*boldItalicFont: xft:Menlo:pixelsize=13:bold:italic
URxvt*letterSpace: -1
!URxvt*font: -sgi-screen-medium-r-*-*-16-*-*-*-*-*-*-*
!URxvt*boldFont: -sgi-screen-bold-r-*-*-16-*-*-*-*-*-*-*
URxvt*internalBorder:  16
URxvt*inheritPixmap: false
! URxvt*transparent:   true
! URxvt*shading: 15
URxvt*perl-ext-common: selection-to-clipboard
URxvt*visualBell: true

*scrollColor:        	#FFFFFF
*cursorBlink:			1

