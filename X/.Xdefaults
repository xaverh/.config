Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

x11-ssh-askpass*font:                   -sgi-screen-medium-r-*-*-13-*-*-*-*-*-*-*
x11-ssh-askpass*background:             #000000
x11-ssh-askpass*foreground:             #ffffff
x11-ssh-askpass.Button*background:      #000000
x11-ssh-askpass.Indicator*foreground:   #ff9900
x11-ssh-askpass.Indicator*background:   #090909
x11-ssh-askpass*topShadowColor:         #000000
x11-ssh-askpass*bottomShadowColor:      #000000
x11-ssh-askpass.*borderWidth:           1

URxvt.intensityStyles: true
URxvt*geometry: 80x24
URxvt*scrollBar: false
! URxvt*letterSpace: -1
! URxvt*font: -sgi-screen-medium-r-*-*-16-*-*-*-*-*-*-*
! URxvt*boldFont: -sgi-screen-bold-r-*-*-16-*-*-*-*-*-*-*
URxvt*font: xft:Iosifovich Terminal:style=Medium,Regular:pixelsize=16:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
URxvt*boldFont: xft:Iosifovich Terminal:style=Bold:pixelsize=16:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
URxvt*italicFont: xft:Iosifovitch Terminal:style=Medium,Italic:pixelsize=16:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
URxvt*boldItalicFont: xft:Iosifovich Terminal:style=Bold Italic:pixelsize=16:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
URxvt*internalBorder:  16
URxvt*inheritPixmap: false
URxvt*transparent: false
URxvt*visualBell: false
URxvt*saveLines: 10000
URxvt.fading: 10
URxvt.fadeColor: #1e1e1e

URxvt*cursorBlink: 1

URxvt.background: #1E1E1E
URxvt.color0:     #1E1E1E
URxvt.color1:     #E32791
URxvt.color2:     #30C798
URxvt.color3:     #E3C472
