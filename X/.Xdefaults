Xft.autohint: false
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! x11-ssh-askpass*font:                   -xos4-terminus-medium-r-normal--14-140-72-72-c-80-iso10646-1
x11-ssh-askpass.Button*background:      #000000
x11-ssh-askpass.Indicator*foreground:   #ff9900
x11-ssh-askpass.Indicator*background:   #090909
x11-ssh-askpass*topShadowColor:         #000000
x11-ssh-askpass*bottomShadowColor:      #000000
x11-ssh-askpass.*borderWidth:           1

URxvt.intensityStyles: false
URxvt*geometry: 80x24
URxvt*scrollBar: false
! URxvt*font: -xos4-terminus-medium-r-normal--14-140-72-72-c-80-iso10646-1
! URxvt*boldFont: -xos4-terminus-bold-r-normal--14-140-72-72-c-80-iso10646-1
URxvt*letterSpace: -1
URxvt*font: xft:SF Mono:style=Regular:pixelsize=13:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
URxvt*boldFont: xft:SF Mono:style=Bold:pixelsize=13:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
URxvt*italicFont: xft:SF Mono:style=Italic:pixelsize=13:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
URxvt*boldItalicFont: xft:SF Mono:style=Bold Italic:pixelsize=13:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
URxvt.internalBorder:  13
URxvt.inheritPixmap: false
URxvt.transparent: false
URxvt.visualBell: false
URxvt.saveLines: 65536
! URxvt.fading: 10
URxvt.cursorBlink: 1
URxvt.iconFile: /usr/share/icons/gnome/32x32/apps/utilities-terminal.png

