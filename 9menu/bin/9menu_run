#!/usr/bin/zsh

9menu -font "-sgi-screen-medium-r-normal--14-140-72-72-m-70-iso8859-1" -bg "#1e1e1e" -fg "#f5f6f6" "$@" \
"9menu:true" \
"---:true" \
"rxvt-unicode:exec urxvtc" \
"tmux:exec urxvtc -e tmux" \
"nnn:exec urxvtc -name nnn -e nnn" \
"Firefox:exec firefox" \
"Google Chrome Dev:exec chrome" \
"WeeChat:exec urxvtc -name WeeChat -e weechat" \
"Visual Studio Code:exec code -n" \
"Zathura:exec zathura" \
"Strawberry:exec strawberry" \
"mpv:exec mpv --player-operation-mode=pseudo-gui --keep-open=yes" \
"GIMP:exec gimp" \
"Steam:exec steam" \
"Change Wallpaper:exec change_wallpaper.sh" \
"Connect Set"$'\xFA'"bal:exec bluetoothctl connect 88:C6:26:F4:8A:90" \
"Screenshot:exec maim ~/tmp/`date +%s`.png" \
"---:true" \
exit

