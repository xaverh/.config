{
	"editor.fontFamily": "Input, Consolas, SF Mono, Menlo, Source Code Pro",
	"editor.fontSize": 16,
	"terminal.integrated.fontSize": 16,
	"editor.rulers": [],
	"editor.formatOnType": true,
	"editor.scrollBeyondLastLine": false,
	"editor.acceptSuggestionOnEnter": "off",
	"editor.wordWrap": "off",
	"editor.wrappingIndent": "same",
	"editor.tabSize": 8,
	"editor.insertSpaces": false,
	"editor.cursorBlinking": "smooth",
	"editor.cursorStyle": "line",
	"editor.renderLineHighlight": "all",
	"editor.renderControlCharacters": true,
	"files.trimTrailingWhitespace": true,
	"files.autoSave": "afterDelay",
	"editor.fontLigatures": true,
	"editor.renderWhitespace": "none",
	"editor.formatOnSave": false,
	"files.associations": {
		"*.clang-format": "yaml",
		"autostart": "shellscript",
		"*.npmrc": "ini",
		"*.service": "ini",
		"*.h": "c",
		"*.ymp": "xml"
	},
	"workbench.editor.showTabs": false,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/._*": true,
		"**/*.un~": true
	},
	"git.confirmSync": false,
	"window.zoomLevel": 0,
	"zenMode.fullScreen": false,
	"workbench.statusBar.visible": true,
	"C_Cpp.formatting": "Disabled",
	"window.menuBarVisibility": "toggle",
	"workbench.iconTheme": null,
	"editor.minimap.enabled": true,
	"editor.dragAndDrop": true,
	"editor.minimap.renderCharacters": false,
	"terminal.external.linuxExec": "st",
	"[html]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[json]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[javascript]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[python]": {
		"editor.tabSize": 4,
		"editor.insertSpaces": true,
		"editor.renderWhitespace": "boundary"
	},
	"[scss]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[typescript]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"workbench.settings.openDefaultSettings": false,
	"clang-format.language.typescript.enable": true,
	"clang-format.language.javascript.enable": false,
	"editor.detectIndentation": true,
	"typescript.tsserver.log": "verbose",
	"debug.inlineValues": true,
	"C_Cpp.autocomplete": "Default",
	"C_Cpp.intelliSenseEngine": "Default",
	"javascript.implicitProjectConfig.checkJs": true,
	"typescript.format.enable": false,
	"extensions.autoUpdate": false,
	"editor.renderIndentGuides": false,
	"editor.autoIndent": true,
	"window.restoreWindows": "all",
	"git.enableSmartCommit": true,
	"markdown.preview.fontSize": 18,
	"workbench.startupEditor": "newUntitledFile",
	"editor.smoothScrolling": true,
	"git.enableCommitSigning": false,
	"workbench.colorTheme": "Qillqaq",
	"workbench.panel.location": "bottom",
	"explorer.decorations.badges": false,
	"explorer.confirmDelete": false
}