{
	"editor.fontFamily": "IBM Plex Mono Text, Input, Consolas, SF Mono, Menlo, Source Code Pro",
	"editor.fontSize": 15,
	"terminal.integrated.fontSize": 14,
	"editor.rulers": [],
	"editor.formatOnType": true,
	"editor.scrollBeyondLastLine": false,
	"editor.acceptSuggestionOnEnter": "off",
	"editor.wordWrap": "on",
	"editor.wrappingIndent": "same",
	"editor.tabSize": 8,
	"editor.insertSpaces": false,
	"editor.cursorBlinking": "smooth",
	"editor.cursorStyle": "line",
	"editor.renderLineHighlight": "all",
	"editor.renderControlCharacters": true,
	"files.trimTrailingWhitespace": true,
	"files.autoSave": "afterDelay",
	"editor.renderWhitespace": "none",
	"editor.formatOnSave": false,
	"files.associations": {
		"*.clang-format": "yaml",
		"autostart": "shellscript",
		"*.npmrc": "ini",
		"*.service": "ini",
		"*.h": "c",
		"*.ymp": "xml"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/._*": true,
		"**/*.un~": true
	},
	"git.confirmSync": false,
	"window.zoomLevel": 0,
	"workbench.statusBar.visible": true,
	"C_Cpp.formatting": "Disabled",
	"window.menuBarVisibility": "toggle",
	"editor.minimap.enabled": false,
	"editor.dragAndDrop": true,
	"editor.minimap.renderCharacters": false,
	"terminal.external.linuxExec": "st",
	"[html]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[json]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[jsonc]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[javascript]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[markdown]": {
		"editor.wordWrap": "on",
		"editor.quickSuggestions": true
	},
	"[python]": {
		"editor.tabSize": 4,
		"editor.insertSpaces": true,
		"editor.renderWhitespace": "boundary"
	},
	"[scss]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[typescript]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"[vue]": {
		"editor.tabSize": 2,
		"editor.insertSpaces": true
	},
	"workbench.settings.openDefaultSettings": false,
	"clang-format.language.typescript.enable": true,
	"clang-format.language.javascript.enable": false,
	"editor.detectIndentation": true,
	"typescript.tsserver.log": "verbose",
	"debug.inlineValues": true,
	"C_Cpp.autocomplete": "Default",
	"C_Cpp.intelliSenseEngine": "Default",
	"javascript.implicitProjectConfig.checkJs": true,
	"typescript.format.enable": false,
	"extensions.autoUpdate": false,
	"editor.renderIndentGuides": false,
	"editor.autoIndent": true,
	"window.restoreWindows": "all",
	"git.enableSmartCommit": true,
	"markdown.preview.fontSize": 18,
	"workbench.startupEditor": "welcomePageInEmptyWorkbench",
	"editor.smoothScrolling": true,
	"git.enableCommitSigning": false,
	"explorer.decorations.badges": false,
	"explorer.confirmDelete": false,
	"git.autofetch": true,
	"javascript.format.enable": false,
	"javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		{
			"language": "html",
			"autoFix": true
		},
		{
			"language": "vue",
			"autoFix": true
		}
	],
	"eslint.options": {
		"plugins": [
			"vue",
			"html"
		]
	},
	"settings.cycle": [
		{
			"id": "colorTheme",
			"overrideWorkspaceSettings": false,
			"values": [
				{
					"workbench.colorTheme": "Qillqaq"
				},
				{
					"workbench.colorTheme": "Ysgrifennwr"
				}
			]
		}
	],
	"search.smartCase": true,
	"terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
	"search.location": "panel",
	"latex-workshop.view.pdf.viewer": "tab",
	"eslint.enable": true,
	"eslint.run": "onType",
	"vetur.format.defaultFormatter.js": "vscode-typescript",
	"vetur.format.defaultFormatter.ts": "vscode-typescript",
	"vetur.validation.style": true,
	"vetur.validation.template": true,
	"debug.toolBarLocation": "docked",
	"latex-workshop.message.update.show": false,
	"workbench.list.keyboardNavigation": "filter",
	"search.usePCRE2": true,
	"workbench.useExperimentalGridLayout": true,
	"latex-workshop.latex.autoBuild.run": "never",
	"workbench.colorTheme": "Qillqaq",
	"workbench.sideBar.location": "left",
	"breadcrumbs.enabled": true,
	"workbench.editor.highlightModifiedTabs": true,
	"window.titleBarStyle": "native",
	"breadcrumbs.symbolPath": "on",
	"diffEditor.ignoreTrimWhitespace": true,
	"window.openWithoutArgumentsInNewWindow": "off",
	"window.openFoldersInNewWindow": "off",
	"files.autoSaveDelay": 1001,
	"zenMode.centerLayout": false,
	"zenMode.hideLineNumbers": false,
	"workbench.activityBar.visible": true
}
