#!/usr/bin/zsh

resizestep=0.02
mutecmd=(spawn pactl set-sink-mute @DEFAULT_SINK@ true)
lowervolumecmd=(chain 🔗 spawn pactl set-sink-mute @DEFAULT_SINK@ false 🔗 spawn pactl set-sink-volume @DEFAULT_SINK@ -5%)
raisevolumecmd=(spawn $SHELL -c "pactl set-sink-mute @DEFAULT_SINK@ false; [[ \$(pacmd list-sinks | grep -A 15 '* index' | awk '/volume: front/{gsub(\"%\",\"\",\$5); print \$5 }') -lt 95 ]] && pactl set-sink-volume @DEFAULT_SINK@ +5% || (pactl set-sink-volume @DEFAULT_SINK@ 65535; pactl set-sink-volume @DEFAULT_SINK@ 65536)")
musicplaycmd=(strawberry -t)
musicnextcmd=(strawberry -f)
musicprevcmd=(strawberry --restart-or-previous)
musicvolupcmd=(strawberry --volume-up)
musicvoldowncmd=(strawberry --volume-down)
qillqaqify=(chain \
💱 try unsetenv IS_YSGRIFENNWR \
💱 spawn sed -i 's/Ysgrifennwr/Qillqaq/' "$HOME/.config/Code/User/settings.json" \
💱 spawn sed -i "s/${YS_K#\#}/${QI_K#\#}/;s/${YS_R#\#}/${QI_R#\#}/;s/${YS_G#\#}/${QI_G#\#}/;s/${YS_B#\#}/${QI_B#\#}/;s/${YS_C#\#}/${QI_C#\#}/;s/${YS_M#\#}/${QI_M#\#}/;s/${YS_Y#\#}/${QI_Y#\#}/;s/${YS_W#\#}/${QI_W#\#}/;s/${YS_B_K#\#}/${QI_B_K#\#}/;s/${YS_B_R#\#}/${QI_B_R#\#}/;s/${YS_B_G#\#}/${QI_B_G#\#}/;s/${YS_B_B#\#}/${QI_B_B#\#}/;s/${YS_B_C#\#}/${QI_B_C#\#}/;s/${YS_B_M#\#}/${QI_B_M#\#}/;s/${YS_B_Y#\#}/${QI_B_Y#\#}/;s/${YS_B_W#\#}/${QI_B_W#\#}/;s/${YS_W_80#\#}/${QI_W_80#\#}/" "$HOME/.config/alacritty/alacritty.yml" "$HOME/.config/termite/config" "$HOME/.config/rofi/config.rasi" "$HOME/etc/X/.Xdefaults" "$HOME/.config/dunst/dunstrc" \
💱 silent try spawn killall -USR1 termite \
💱 silent try spawn killall dunst \
💱 silent try spawn $SHELL -c "sleep .5 && killall -USR1 termite" \
💱 emit_hook 🧛)
ysgrifennwrify=(chain \
💱 setenv IS_YSGRIFENNWR 1 \
💱 spawn sed -i 's/Qillqaq/Ysgrifennwr/' "$HOME/.config/Code/User/settings.json" \
💱 spawn sed -i "s/${QI_K#\#}/${YS_K#\#}/;s/${QI_R#\#}/${YS_R#\#}/;s/${QI_G#\#}/${YS_G#\#}/;s/${QI_B#\#}/${YS_B#\#}/;s/${QI_C#\#}/${YS_C#\#}/;s/${QI_M#\#}/${YS_M#\#}/;s/${QI_Y#\#}/${YS_Y#\#}/;s/${QI_W#\#}/${YS_W#\#}/;s/${QI_B_K#\#}/${YS_B_K#\#}/;s/${QI_B_R#\#}/${YS_B_R#\#}/;s/${QI_B_G#\#}/${YS_B_G#\#}/;s/${QI_B_B#\#}/${YS_B_B#\#}/;s/${QI_B_C#\#}/${YS_B_C#\#}/;s/${QI_B_M#\#}/${YS_B_M#\#}/;s/${QI_B_Y#\#}/${YS_B_Y#\#}/;s/${QI_B_W#\#}/${YS_B_W#\#}/;s/${QI_W_80#\#}/${YS_W_80#\#}/" "$HOME/.config/alacritty/alacritty.yml" "$HOME/.config/termite/config" "$HOME/.config/rofi/config.rasi" "$HOME/etc/X/.Xdefaults" "$HOME/.config/dunst/dunstrc" \
💱 silent try spawn killall -USR1 termite \
💱 silent try spawn killall dunst \
💱 silent try spawn $SHELL -c "sleep .5 && killall -USR1 termite" \
💱 emit_hook 🧚)

if [[ $HOST = andermatt ]] ; then;
	maxbrightness=$(</sys/class/backlight/intel_backlight/max_brightness)
	localbinds=(🍂 or 🔙 silent get_attr my_touchpad_enabled 🔙 new_attr bool my_touchpad_enabled \
	🍂 set_attr my_touchpad_enabled true \
	🍂 keybind XF86TouchpadToggle or 🔙 and 🥨 compare my_touchpad_enabled = true 🥨 spawn xinput disable 'AlpsPS/2 ALPS GlidePoint' 🥨 set_attr my_touchpad_enabled false 🔙 and 🥨 spawn xinput enable 'AlpsPS/2 ALPS GlidePoint' 🥨 set_attr my_touchpad_enabled true \
	🍂 keybind XF86MonBrightnessDown spawn $SHELL -c "brightness=\$(</sys/class/backlight/intel_backlight/brightness); new_brightness=\$((-$maxbrightness/10+\$brightness)); actual_brightness=\$((\$new_brightness > 0 ? \$new_brightness : 0)); echo \$actual_brightness > /sys/class/backlight/intel_backlight/brightness && dunstify -r 5994 \"🔅 \$(( \$(</sys/class/backlight/intel_backlight/brightness) * 100 / $maxbrightness )) %\"" \
	🍂 keybind XF86MonBrightnessUp spawn $SHELL -c "brightness=\$(</sys/class/backlight/intel_backlight/brightness) ; new_brightness=\$(($maxbrightness/10+\$brightness)) ; actual_brightness=\$((\$new_brightness < $maxbrightness ? \$new_brightness : $maxbrightness)) ; echo \$actual_brightness > /sys/class/backlight/intel_backlight/brightness && dunstify -r 5994 \"🔆 \$(( \$(</sys/class/backlight/intel_backlight/brightness) * 100 / $maxbrightness )) %\"" \
	🍂 keybind XF86AudioMute $mutecmd \
	🍂 keybind XF86AudioLowerVolume $lowervolumecmd \
	🍂 keybind XF86AudioRaiseVolume $raisevolumecmd \
	🍂 keybind Mod4+XF86AudioLowerVolume spawn $musicvoldowncmd \
	🍂 keybind Mod4+XF86AudioRaiseVolume spawn $musicvolupcmd \
	🍂 keybind XF86AudioPlay spawn $musicplaycmd \
	🍂 keybind XF86AudioNext spawn $musicnextcmd \
	🍂 keybind XF86AudioPrev spawn $musicprevcmd \
	🍂 keybind Mod4+asciicircum cycle_frame +1 \
	🍂 keybind Mod4+Mod1+asciicircum chain ⛓️ lock ⛓️ rotate ⛓️ rotate ⛓️ rotate ⛓️ unlock \
	🍂 keybind Mod4+Shift+asciicircum substitute 🧓 clients.focus.winid chain ⛓️ lock ⛓️ cycle_frame +1 ⛓️ bring 🧓 ⛓️ unlock \
	🍂 keybind Mod4+less chain ⛓️ split right 0.6180339887498949 ⛓️ focus -e right \
	🍂 keybind Mod4+v substitute 🪟 clients.focus.winid and 🥨 load '(split horizontal:0.612700:0 (clients max:1) (split vertical:0.415300:1 (clients max:0) (clients max:0)))' 🥨 jumpto 🪟 \
	🍂 keybind Mod4+odiaeresis or 🙉 sprintf 🖖 my_%s_🖼️ tags.focus.index substitute 🆔 clients.focus.winid  and 🥨 get_attr 🖖 🥨 substitute 📊 🖖 load 📊 🥨 remove_attr 🖖 🥨 jumpto 🆔 🙉 spawn $SHELL -c "herbstclient sprintf 🖖 my_%s_🖼️ tags.focus.index and 🥨 new_attr string 🖖 🥨 set_attr 🖖 \"\`herbstclient dump\`\" 🥨 load \"\`herbstclient dump '' @\`\"" \
	🍂 keybind Mod4+numbersign and 🥨 toggle always_show_frame 1 🥨 spawn $SHELL -c 'sleep 1 && herbstclient toggle always_show_frame' \
	🍂 keybind Mod4+Shift+numbersign toggle always_show_frame)
else
	localbinds=(🍂 keybind Mod4+grave cycle_frame +1 \
	🍂 keybind Mod4+Mod1+grave chain ⛓️ lock ⛓️ rotate ⛓️ rotate ⛓️ rotate ⛓️ unlock \
	🍂 keybind Mod4+Shift+grave substitute 🧓 clients.focus.winid chain ⛓️ lock ⛓️ cycle_frame +1 ⛓️ bring 🧓 ⛓️ unlock \
	🍂 keybind Mod4+KP_Insert $mutecmd \
	🍂 keybind Mod4+KP_Subtract $lowervolumecmd \
	🍂 keybind Mod4+KP_Add $raisevolumecmd \
	🍂 keybind Mod4+Shift+KP_Subtract spawn $musicvoldowncmd \
	🍂 keybind Mod4+Shift+KP_Add spawn $musicvolupcmd \
	🍂 keybind Mod4+KP_End spawn $musicplaycmd \
	🍂 keybind Mod4+KP_Multiply spawn $musicnextcmd \
	🍂 keybind Mod4+KP_Divide spawn $musicprevcmd \
	🍂 keybind Mod4+backslash chain ⛓️ split right 0.6180339887498949 ⛓️ focus -e right \
	🍂 keybind Mod4+v substitute 🪟 clients.focus.winid and 🥨 load '(split horizontal:0.626100:0 (clients max:1) (split vertical:0.395300:1 (clients max:0) (clients max:0)))' 🥨 jumpto 🪟 \
	🍂 keybind Mod4+semicolon or 🙉 sprintf 🖖 my_%s_🖼️ tags.focus.index substitute 🆔 clients.focus.winid  and 🥨 get_attr 🖖 🥨 substitute 📊 🖖 load 📊 🥨 remove_attr 🖖 🥨 jumpto 🆔 🙉 spawn $SHELL -c "herbstclient sprintf 🖖 my_%s_🖼️ tags.focus.index and 🥨 new_attr string 🖖 🥨 set_attr 🖖 \"\`herbstclient dump\`\" 🥨 load \"\`herbstclient dump '' @\`\"" \
	🍂 keybind Mod4+apostrophe and 🥨 toggle always_show_frame 1 🥨 spawn $SHELL -c 'sleep 1 && herbstclient toggle always_show_frame' \
	🍂 keybind Mod4+Shift+apostrophe toggle always_show_frame)
fi

herbstclient chain \
🍂 emit_hook reload \
🍂 and 🥨 new_attr bool my_backdrop_not_yet_set 🥨 spawn $SHELL -c "xsetroot -bitmap $HOME/.local/share/backdrops/\`\\ls $HOME/.local/share/backdrops | shuf -n 1 | tr -d '\\n' | tee -a /tmp/wallpaper\` \`printf -- \" -fg #%06x -bg #%06x\\n\" \$(shuf -i0-16777215 -n2) | tee -a /tmp/wallpaper\`" \
\
🍂 set frame_gap 15 \
🍂 set frame_padding 0 \
🍂 set window_gap 0 \
🍂 set snap_distance 10 \
🍂 set snap_gap 0 \
🍂 set mouse_recenter_gap 1 \
