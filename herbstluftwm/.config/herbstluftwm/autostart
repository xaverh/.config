#!/usr/bin/zsh

resizestep=0.02
accentcolors=("$QI_G" "$QI_M" "$QI_C" "$QI_Y" "$QI_B")
accentcolor="$accentcolors[RANDOM % $#accentcolors + 1]"
mutecmd=(and 🥨 spawn $SHELL -c "pactl set-sink-mute \$(pacmd list-sinks|awk '/\\* index:/{ print \$3 }') true" 🥨 spawn notify-send '🔇 muted')
lowervolumecmd=(spawn $SHELL -c "sink=\$(pacmd list-sinks | awk '/\\* index:/{ print \$3 }'); pactl set-sink-mute \$(pacmd list-sinks|awk '/\\* index:/{ print \$3 }') false; pactl set-sink-volume \"\$sink\" \"\$(( \$(pacmd list-sinks | grep -A 15 '* index' | awk '/volume: front/{gsub(\"%\",\"\",\$5); print \$5 }') - 5 ))%\" && notify-send \"🔉 \$(pacmd list-sinks | grep -A 15 '* index' | awk '/volume: front/{gsub(\"%\",\"\",\$5); print \$5 }') %\"" )
raisevolumecmd=(spawn $SHELL -c "sink=\$(pacmd list-sinks | awk '/\\* index:/{ print \$3 }'); pactl set-sink-mute \$(pacmd list-sinks|awk '/\\* index:/{ print \$3 }') false; newvol=\$(( \$(pacmd list-sinks | grep -A 15 '* index' | awk '/volume: front/{gsub(\"%\",\"\",\$5); print \$5 }') + 5 )); pactl set-sink-volume \"\$sink\" \"\$(( (\$newvol > 100) ? 100 : \$newvol ))%\" && notify-send \"🔊 \$(pacmd list-sinks | grep -A 15 '* index' | awk '/volume: front/{gsub(\"%\",\"\",\$5); print \$5 }') %\"" )
musicplaycmd=(deadbeef --play-pause)
musicnextcmd=(deadbeef --next)
musicprevcmd=(deadbeef --prev)

if [[ $HOST = andermatt ]] ; then;
	maxbrightness=$(</sys/class/backlight/intel_backlight/max_brightness)
	localbinds=(🍂 or 🔙 silent get_attr my_touchpad_enabled 🔙 new_attr bool my_touchpad_enabled \
	🍂 set_attr my_touchpad_enabled true \
	🍂 keybind XF86TouchpadToggle or 🔙 and 🥨 compare my_touchpad_enabled = true 🥨 spawn xinput disable 'AlpsPS/2 ALPS GlidePoint' 🥨 set_attr my_touchpad_enabled false 🔙 and 🥨 spawn xinput enable 'AlpsPS/2 ALPS GlidePoint' 🥨 set_attr my_touchpad_enabled true \
	🍂 keybind XF86MonBrightnessDown and 🥨 spawn $SHELL -c "brightness=\$(</sys/class/backlight/intel_backlight/brightness); new_brightness=\$((-$maxbrightness/10+\$brightness)); actual_brightness=\$((\$new_brightness > 0 ? \$new_brightness : 0)); echo \$actual_brightness > /sys/class/backlight/intel_backlight/brightness" 🥨 spawn $SHELL -c "notify-send \"🔅 \$(( \$(</sys/class/backlight/intel_backlight/brightness) * 100 / $maxbrightness )) %\"" \
	🍂 keybind XF86MonBrightnessUp and 🥨 spawn $SHELL -c "brightness=\$(</sys/class/backlight/intel_backlight/brightness) ; new_brightness=\$(($maxbrightness/10+\$brightness)) ; actual_brightness=\$((\$new_brightness < $maxbrightness ? \$new_brightness : $maxbrightness)) ; echo \$actual_brightness > /sys/class/backlight/intel_backlight/brightness" 🥨 spawn $SHELL -c "notify-send \"🔆 \$(( \$(</sys/class/backlight/intel_backlight/brightness) * 100 / $maxbrightness )) %\"" \
	🍂 keybind XF86AudioMute $mutecmd \
	🍂 keybind XF86AudioLowerVolume $lowervolumecmd \
	🍂 keybind XF86AudioRaiseVolume $raisevolumecmd \
	🍂 keybind XF86AudioPlay spawn $musicplaycmd \
	🍂 keybind XF86AudioNext spawn $musicnextcmd \
	🍂 keybind XF86AudioPrev spawn $musicprevcmd)
fi

herbstclient chain \
🍂 emit_hook reload \
🍂 keyunbind --all \
🍂 mouseunbind --all \
🍂 unrule --all \
🍂 and 🥨 silent new_attr color my_🦎 🥨 set_attr my_🦎 `printf -- "#%06x" $(shuf -i0-16777215 -n1)` \
\
🍂 set frame_gap 15 \
🍂 set frame_padding 0 \
🍂 set window_gap 0 \
🍂 set snap_distance 10 \
🍂 set snap_gap 0 \
🍂 set mouse_recenter_gap 1 \
🍂 set frame_border_active_color "$YS_B_C" \
🍂 set frame_border_normal_color "$YS_B_K" \
🍂 set frame_bg_active_color "$YS_C" \
🍂 set frame_bg_normal_color "$YS_B_K" \
🍂 set frame_bg_transparent 1 \
🍂 set frame_transparent_width 0 \
🍂 set frame_border_width 5 \
🍂 set frame_border_inner_width 0 \
🍂 set focus_crosses_monitor_boundaries 1 \
🍂 set raise_on_focus 0 \
🍂 set raise_on_focus_temporarily 0 \
🍂 set raise_on_click 1 \
🍂 set window_border_width 5 \
🍂 set window_border_inner_width 0 \
🍂 set window_border_active_color "$YS_C" \
🍂 set window_border_normal_color '#696969' \
🍂 set window_border_urgent_color "$QI_R" \
🍂 set always_show_frame 0 \
🍂 set default_frame_layout 2 \
🍂 set default_direction_external_only 0 \
🍂 set gapless_grid 1 \
🍂 set smart_frame_surroundings 1 \
🍂 set smart_window_surroundings 1 \
🍂 set focus_follows_mouse 0 \
🍂 set focus_stealing_prevention 0 \
🍂 set swap_monitors_to_get_tag 1 \
🍂 set auto_detect_monitors 0 \
🍂 set tree_style '├│ ├└──┬' \
🍂 set update_dragged_clients 1 \
\
🍂 keybind Mod4+Left  focus left \
🍂 keybind Mod4+Down  focus down \
🍂 keybind Mod4+Up    focus up \
🍂 keybind Mod4+Right focus right \
🍂 keybind Mod4+h     focus left \
🍂 keybind Mod4+j     focus down \
🍂 keybind Mod4+k     focus up \
🍂 keybind Mod4+l     focus right \
🍂 keybind Mod4-Shift-Left or / shift left / chain , lock , shift_to_monitor -l , focus_monitor -l , unlock \
🍂 keybind Mod4-Shift-Down or / shift down / chain , lock , shift_to_monitor -d , focus_monitor -d , unlock \
🍂 keybind Mod4-Shift-Up or / shift up / chain , lock , shift_to_monitor -u , focus_monitor -u , unlock \
🍂 keybind Mod4-Shift-Right or / shift right / chain , lock , shift_to_monitor -r , focus_monitor -r , unlock \
🍂 keybind Mod4-Shift-h or / shift left / chain , lock , shift_to_monitor -l , focus_monitor -l , unlock \
🍂 keybind Mod4-Shift-j or / shift down / chain , lock , shift_to_monitor -d , focus_monitor -d , unlock \
🍂 keybind Mod4-Shift-k or / shift up / chain , lock , shift_to_monitor -u , focus_monitor -u , unlock \
🍂 keybind Mod4-Shift-l or / shift right / chain , lock , shift_to_monitor -r , focus_monitor -r , unlock \
🍂 keybind Mod4+backslash chain ⛓️ split right 0.6180339887498949 ⛓️ focus -e right \
🍂 keybind Mod4+less chain ⛓️ split right 0.6180339887498949 ⛓️ focus -e right \
🍂 keybind Mod4+minus chain ⛓️ split bottom 0.6180339887498949 ⛓️ focus -e down \
🍂 keybind Mod4+x split explode \
🍂 keybind Mod4+Control+h       resize left +$resizestep \
🍂 keybind Mod4+Control+j       resize down +$resizestep \
🍂 keybind Mod4+Control+k       resize up +$resizestep \
🍂 keybind Mod4+Control+l       resize right +$resizestep \
🍂 keybind Mod4+Control+Left    resize left +$resizestep \
🍂 keybind Mod4+Control+Down    resize down +$resizestep \
🍂 keybind Mod4+Control+Up      resize up +$resizestep \
🍂 keybind Mod4+Control+Right   resize right +$resizestep \
🍂 keybind Mod4+Mod1+Right substitute 🔛 clients.focus.winid chain ⛓️ lock ⛓️ focus -e right ⛓️ or 🙉 shift -e left 🙉 chain 🔗 shift_to_monitor -l 🔗 focus_monitor -l ⛓️ focus -e right ⛓️ bring 🔛 ⛓️ unlock \
🍂 keybind Mod4+Mod1+Left substitute 🔛 clients.focus.winid chain ⛓️ lock ⛓️ focus -e left ⛓️ or 🙉 shift -e right 🙉 chain 🔗 shift_to_monitor -r 🔗 focus_monitor -r ⛓️ focus -e left ⛓️ bring 🔛 ⛓️ unlock \
🍂 keybind Mod4+Mod1+Up substitute 🔛 clients.focus.winid chain ⛓️ lock ⛓️ focus -e up ⛓️ or 🙉 shift -e down 🙉 chain 🔗 shift_to_monitor -d 🔗 focus_monitor -d ⛓️ focus -e up ⛓️ bring 🔛 ⛓️ unlock \
🍂 keybind Mod4+Mod1+Down substitute 🔛 clients.focus.winid chain ⛓️ lock ⛓️ focus -e down ⛓️ or 🙉 shift -e up 🙉 chain 🔗 shift_to_monitor -u 🔗 focus_monitor -u ⛓️ focus -e down ⛓️ bring 🔛 ⛓️ unlock \
🍂 keybind Mod4+Mod1+l substitute 🔛 clients.focus.winid chain ⛓️ lock ⛓️ focus -e right ⛓️ or 🙉 shift -e left 🙉 chain 🔗 shift_to_monitor -l 🔗 focus_monitor -l ⛓️ focus -e right ⛓️ bring 🔛 ⛓️ unlock \
🍂 keybind Mod4+Mod1+h substitute 🔛 clients.focus.winid chain ⛓️ lock ⛓️ focus -e left ⛓️ or 🙉 shift -e right 🙉 chain 🔗 shift_to_monitor -r 🔗 focus_monitor -r ⛓️ focus -e left ⛓️ bring 🔛 ⛓️ unlock \
🍂 keybind Mod4+Mod1+k substitute 🔛 clients.focus.winid chain ⛓️ lock ⛓️ focus -e up ⛓️ or 🙉 shift -e down 🙉 chain 🔗 shift_to_monitor -d 🔗 focus_monitor -d ⛓️ focus -e up ⛓️ bring 🔛 ⛓️ unlock \
🍂 keybind Mod4+Mod1+j substitute 🔛 clients.focus.winid chain ⛓️ lock ⛓️ focus -e down ⛓️ or 🙉 shift -e up 🙉 chain 🔗 shift_to_monitor -u 🔗 focus_monitor -u ⛓️ focus -e down ⛓️ bring 🔛 ⛓️ unlock \
\
🍂 silent rename default '!' \
🍂 add '@' \
🍂 add '#' \
🍂 add '$' \
🍂 add '%' \
🍂 add '^' \
🍂 load '!' '(clients grid:0)' \
🍂 load '@' '(split horizontal:0.626100:0 (clients max:1) (split vertical:0.395300:1 (clients max:0) (clients max:0)))' \
🍂 load '#' '(clients grid:0)' \
🍂 load '%' '(split horizontal:0.550000:0 (clients max:0) (clients max:0))' \
🍂 keybind Mod4+1 use_index 0 \
🍂 keybind Mod4+2 use_index 1 \
🍂 keybind Mod4+3 use_index 2 \
🍂 keybind Mod4+4 use_index 3 \
🍂 keybind Mod4+5 use_index 4 \
🍂 keybind Mod4+6 use_index 5 \
🍂 keybind Mod4+Shift+1 move_index 0 \
🍂 keybind Mod4+Shift+2 move_index 1 \
🍂 keybind Mod4+Shift+3 move_index 2 \
🍂 keybind Mod4+Shift+4 move_index 3 \
🍂 keybind Mod4+Shift+5 move_index 4 \
🍂 keybind Mod4+Shift+6 move_index 5 \
🍂 keybind Mod4+Mod1+1 substitute 🔜 tags.focus.name chain ⛓️ lock ⛓️ use_index 0 ⛓️ move 🔜 ⛓️ use 🔜 ⛓️ unlock \
🍂 keybind Mod4+Mod1+2 substitute 🔜 tags.focus.name chain ⛓️ lock ⛓️ use_index 1 ⛓️ move 🔜 ⛓️ use 🔜 ⛓️ unlock \
🍂 keybind Mod4+Mod1+3 substitute 🔜 tags.focus.name chain ⛓️ lock ⛓️ use_index 2 ⛓️ move 🔜 ⛓️ use 🔜 ⛓️ unlock \
🍂 keybind Mod4+Mod1+4 substitute 🔜 tags.focus.name chain ⛓️ lock ⛓️ use_index 3 ⛓️ move 🔜 ⛓️ use 🔜 ⛓️ unlock \
🍂 keybind Mod4+Mod1+5 substitute 🔜 tags.focus.name chain ⛓️ lock ⛓️ use_index 4 ⛓️ move 🔜 ⛓️ use 🔜 ⛓️ unlock \
🍂 keybind Mod4+Mod1+6 substitute 🔜 tags.focus.name chain ⛓️ lock ⛓️ use_index 5 ⛓️ move 🔜 ⛓️ use 🔜 ⛓️ unlock \
🍂 keybind Mod4+7 or 🙈 substitute 🆔 my_7️⃣ or 💀 jumpto 🆔 💀 substitute 🆕 clients.focus.winid set_attr my_7️⃣ 🆕 🙈 chain ⛓️ new_attr string my_7️⃣ ⛓️ substitute 🆕 clients.focus.winid set_attr my_7️⃣ 🆕 \
🍂 keybind Mod4+8 or 🙈 substitute 🆔 my_8️⃣ or 💀 jumpto 🆔 💀 substitute 🆕 clients.focus.winid set_attr my_8️⃣ 🆕 🙈 chain ⛓️ new_attr string my_8️⃣ ⛓️ substitute 🆕 clients.focus.winid set_attr my_8️⃣ 🆕 \
🍂 keybind Mod4+9 or 🙈 substitute 🆔 my_9️⃣ or 💀 jumpto 🆔 💀 substitute 🆕 clients.focus.winid set_attr my_9️⃣ 🆕 🙈 chain ⛓️ new_attr string my_9️⃣ ⛓️ substitute 🆕 clients.focus.winid set_attr my_9️⃣ 🆕 \
🍂 keybind Mod4+Mod1+8 substitute 🆔 my_8️⃣ bring 🆔 \
🍂 keybind Mod4+Mod1+7 substitute 🆔 my_7️⃣ bring 🆔 \
🍂 keybind Mod4+Mod1+9 substitute 🆔 my_9️⃣ bring 🆔 \
🍂 keybind Mod4+Control+Shift+7 substitute 🆔 my_7️⃣ close 🆔 \
🍂 keybind Mod4+Control+Shift+8 substitute 🆔 my_8️⃣ close 🆔 \
🍂 keybind Mod4+Control+Shift+9 substitute 🆔 my_9️⃣ close 🆔 \
🍂 keybind Mod4+Shift+7 chain ⛓️ new_attr string my_7️⃣ ⛓️ substitute 🆕 clients.focus.winid set_attr my_7️⃣ 🆕 \
🍂 keybind Mod4+Shift+8 chain ⛓️ new_attr string my_8️⃣ ⛓️ substitute 🆕 clients.focus.winid set_attr my_8️⃣ 🆕 \
🍂 keybind Mod4+Shift+9 chain ⛓️ new_attr string my_9️⃣ ⛓️ substitute 🆕 clients.focus.winid set_attr my_9️⃣ 🆕 \
🍂 keybind Mod4+r remove \
🍂 keybind Mod4+Shift+f floating toggle \
🍂 keybind Mod4+f fullscreen toggle \
🍂 keybind Mod4+Control+Shift+f pseudotile toggle \
