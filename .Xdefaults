Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

x11-ssh-askpass*font:                   -sgi-screen-medium-r-*-*-13-*-*-*-*-*-*-*
x11-ssh-askpass*background:             #000000
x11-ssh-askpass*foreground:             #ffffff
x11-ssh-askpass.Button*background:      #000000
x11-ssh-askpass.Indicator*foreground:   #ff9900
x11-ssh-askpass.Indicator*background:   #090909
x11-ssh-askpass*topShadowColor:         #000000
x11-ssh-askpass*bottomShadowColor:      #000000
x11-ssh-askpass.*borderWidth:           1

URxvt.intensityStyles: true
URxvt*geometry: 80x24
URxvt*scrollBar: false
! URxvt*letterSpace: -1
URxvt*font: -sgi-screen-medium-r-normal--14-140-72-72-m-70-iso8859-1
URxvt*boldFont: -sgi-screen-bold-r-normal--14-140-72-72-m-80-iso8859-1
! URxvt*font: xft:IBM Plex Mono Text:style=Regular:pixelsize=15:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
! URxvt*boldFont: xft:IBM Plex Mono:style=Bold:pixelsize=15:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
! URxvt*italicFont: xft:IBM Plex Mono Text:style=Italic:pixelsize=15:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
! URxvt*boldItalicFont: xft:IBM Plex Mono:style=Bold Italic:pixelsize=15:antialias=true:hintstyle=1:lcdfilter=1:rgba=1
URxvt.internalBorder:  15
URxvt.inheritPixmap: false
URxvt.transparent: false
URxvt.visualBell: false
URxvt.saveLines: 10000
URxvt.fading: 10
URxvt.cursorBlink: 1

